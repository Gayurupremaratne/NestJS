{
  "name": "admin-backend",
  "version": "1.4.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": "20.11.0"
  },
  "scripts": {
    "build": "nest build",
    "postbuild": "node dist/src/common/helpers/mjml-parser.js",
    "mail:templates": "npx ts-node src/common/helpers/mjml-parser.ts",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "dotenv -e .env.test -- prisma migrate deploy && dotenv -e .env.test -- prisma generate && dotenv -e .env.test --  prisma db seed && dotenv -e .env.test -- node --expose-gc --no-compilation-cache -- ./node_modules/.bin/jest --runInBand --logHeapUsage --workerIdleMemoryLimit='500MB'",
    "test:watch": "jest --runInBand --watch",
    "test:cov": "jest --runInBand --coverage --logHeapUsage --workerIdleMemoryLimit='2GB'",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.32.0",
    "@aws-sdk/s3-request-presigner": "^3.385.0",
    "@casl/ability": "^6.5.0",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/bull": "^10.0.1",
    "@nestjs/common": "^10.1.3",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.1.3",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/platform-express": "^10.2.1",
    "@nestjs/schedule": "^3.0.4",
    "@nestjs/swagger": "^7.0.10",
    "@nestjs/throttler": "^5.0.1",
    "@prisma/client": "^5.1.1",
    "@sentry/node": "^7.64.0",
    "@sentry/tracing": "^7.64.0",
    "@tmcw/togeojson": "^5.8.1",
    "@travelerdev/nestjs-sentry": "^4.2.1",
    "@types/bull": "^4.10.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/multer": "^1.4.7",
    "bull": "^4.11.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "country-list": "^2.3.0",
    "dotenv-cli": "^7.2.1",
    "firebase-admin": "^11.10.1",
