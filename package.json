    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "mustache": "^4.2.0",
    "nest-casl": "^1.8.11",
    "nest-keycloak-connect": "^1.9.2",
    "nestjs-i18n": "10.2.6",
    "nestjs-throttler-storage-redis": "^0.4.1",
