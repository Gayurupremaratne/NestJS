  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@mermaid-js/mermaid-cli": "^10.4.0",
    "@nestjs/cli": "^10.1.11",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.1.3",
    "@sentry/types": "^7.64.0",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/mjml": "^4.7.1",
    "@types/node": "18.17.1",
    "@types/nodemailer": "^6.4.9",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "29.6.2",
    "prettier": "^3.0.1",
    "prisma": "^5.1.1",
    "prisma-erd-generator": "^1.11.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.1.1",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.2.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testTimeout": 120000,
    "coverageProvider": "v8",
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@common/(.*)$": "<rootDir>/common/$1",
      "^@config/(.*)$": "<rootDir>/config/$1",
      "^@auth/(.*)$": "<rootDir>/auth/$1",
      "^@keycloak/(.*)$": "<rootDir>/keycloak/$1",
      "^@prisma-config/(.*)$": "<rootDir>/../prisma/$1",
      "^@prisma-orm/(.*)$": "<rootDir>/prisma/$1",
      "^@user/(.*)$": "<rootDir>/modules/user/$1",
      "^@policies/(.*)$": "<rootDir>/modules/policies/$1"
    },
    "setupFiles": [
      "<rootDir>/../jest.setup.ts"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/common/",
      "<rootDir>/modules/casl/",
      "<rootDir>/config/",
      "<rootDir>/prisma/",
      "<rootDir>/modules/keycloak/",
      "<rootDir>/worker/",
      ".dto.ts",
      "main.ts",
      "index.ts"
    ]
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "overrides": {
    "strip-ansi": "7.1.0"
  }
}